    "def read_file(filename):\n",
    "    raw_data = []\n",
    "    file = open(filename, encoding = 'utf8')\n",
    "    for line in file:\n",
    "        line = line.rstrip()\n",
    "        raw_data.append(line)\n",
    "    return raw_data"
   ]
